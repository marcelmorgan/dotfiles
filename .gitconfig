[color]
	ui = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow
	frag = magenta
	old = red bold
	new = green
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[alias]
	a = add
	s = status
	c = commit -v
	b = branch
	m = merge
	mc = merge --continue
	ma = merge --abort
	ms = merge --skip
	r = rebase
	rc = rebase --continue
	ra = rebase --abort
	rs = rebase --skip
	cp = cherry-pick
	cpc = cherry-pic --continue
	cpa = cherry-pick --abort
	p = push
	pu = push --set-upstream origin
	pff = pull --ff-only
	pr = pull --rebase
	f = fetch
	d = diff --color-words -w
	ds = diff --color-words -w --staged
	l = log -p
	co = checkout
	fix = commit --amend -C HEAD
	rs = remote show
	ls = ls-files
	sl = shortlog -sn
	prune = !sh -c 'git branch | grep -v "master" | xargs git branch -D'
[core]
	editor = vim
	excludesfile = /Users/marcel/.gitignore
	autocrlf = input
[branch]
	autosetuprebase = always
[branch "master"]
	mergeoptions = --no-commit --ff-only
[push]
	default = current
[help]
	format = web
[init]
	templatedir = ~/.git-templates
[user]
	name = Marcel Morgan
	email = 371739+mcmorgan@users.noreply.github.com
