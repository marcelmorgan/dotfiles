# User configuration

# Set lang correctly so that we don't have prompt issues
export LC_ALL=en_US.UTF-8
export LANG=en_US.UTF-8

# Adding project-specific binstubs to PATH
export PATH="./bin:$PATH"

export TERM="screen-256color"
export EDITOR=vim
export FIVEMAT_PROFILE=1

alias v='vim'
alias r='rails'
alias c='cap'
alias b='bundle'
alias t='tmux'
alias g='git'
alias h='heroku'

