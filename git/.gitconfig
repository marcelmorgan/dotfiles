[color]
	ui = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow
	frag = magenta
	old = red bold
	new = green
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[alias]
	a = add
	b = branch
	c = commit -v
	co = checkout
	cp = cherry-pick
	cpa = cherry-pick --abort
	cpc = cherry-pic --continue
	d = diff --color-words -w
	ds = diff --color-words -w --staged
	f = fetch
	fix = commit --amend -C HEAD
	l = log -p
	ls = ls-files
	m = merge
	ma = merge --abort
	mc = merge --continue
	ms = merge --skip
	p = push
	pb = !sh -c 'git branch | grep -v master | xargs git branch -D'
	pff = pull --ff-only
	pr = pull --rebase
	r = rebase
	rc = rebase --continue
	ra = rebase --abort
	rs = rebase --skip
	s = status
	sl = shortlog -sn
[core]
	editor = vim
	excludesfile = /Users/marcel/.gitignore
	autocrlf = input
[branch]
	autosetuprebase = always
[branch "master"]
	mergeoptions = --no-commit --ff-only
[push]
	default = current
[help]
	format = web
[init]
	templatedir = ~/.git-templates
	defaultBranch = main
[user]
	name = Marcel Morgan
	email = 371739+mcmorgan@users.noreply.github.com
